<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ShowPage</servlet-name>
  	<display-name>ShowPage</display-name>
  	<description></description>
  	<servlet-class>com.biz.servlet.ShowPage</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>QueryByPage</servlet-name>
  	<display-name>QueryByPage</display-name>
  	<description></description>
  	<servlet-class>com.biz.servlet.QueryByPage</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>InsertServlet</servlet-name>
  	<display-name>InsertServlet</display-name>
  	<description></description>
  	<servlet-class>com.biz.servlet.InsertServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateServlet</servlet-name>
  	<display-name>UpdateServlet</display-name>
  	<description></description>
  	<servlet-class>com.biz.servlet.UpdateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ShowPage</servlet-name>
  	<url-pattern>/ShowPage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>QueryByPage</servlet-name>
  	<url-pattern>/QueryByPage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>InsertServlet</servlet-name>
  	<url-pattern>/InsertServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateServlet</servlet-name>
  	<url-pattern>/UpdateServlet</url-pattern>
  </servlet-mapping>
  
</web-app>
